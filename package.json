{
  "name": "globalgate-app",
  "version": "3.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "webpack --config webpack.common.js --watch",
    "build": "webpack --config webpack.prod.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "make:win": "electron-builder --win",
    "make:macos": "electron-builder --mac"
  },
  "author": "GG SARL(KASONGA MBAYI DANIEL)",
  "license": "ISC",
  "build": {
    "productName": "Global Gate Setup 3.0.0",
    "appId": "com.globalgate.app",
    "files": [
      "build/**/*",
      "src/**/*",
      "Icon.png",
      "index.html",
      "main.js",
      "preload.js"
    ],
    "win": {
      "icon": "Icon.png",
      "target": [
        "NSIS"
      ]
    },
    "mac": {
      "category": "public.app-category.business",
      "icon": "Icon.png",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 430,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "devDependencies": {
    "electron": "^26.6.1",
    "electron-builder": "^24.9.1",
    "electron-reloader": "^1.2.3",
    "webpack-merge": "^5.9.0"
  },
